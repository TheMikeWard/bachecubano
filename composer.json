{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "anlutro/l4-settings": "^0.8.3",
        "artesaos/seotools": "^0.14.0",
        "binarytorch/larecipe": "^2.2",
        "biscolab/laravel-recaptcha": "^3.5",
        "facebook/graph-sdk": "^5.7",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "laravel-notification-channels/facebook": "^0.0.3",
        "laravel-notification-channels/facebook-poster": "^1.5",
        "laravel-notification-channels/telegram": "^0.0.5",
        "laravel-notification-channels/twitter": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "overtrue/laravel-like": "^1.0",
        "php-mail-client/client": "^3.2",
        "rennokki/befriended": "^1.3",
        "rennokki/rating": "^1.1",
        "spatie/laravel-responsecache": "^5.0",
        "spatie/laravel-sitemap": "^5.3",
        "spatie/schema-org": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan optimize",
            "@php artisan responsecache:clear",
            "@php artisan cache:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
